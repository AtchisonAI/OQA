<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IWcfContract" />
                <binding name="BasicHttpBinding_IOQAContract" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8733/Design_Time_Addresses/WcfService/"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IWcfContract"
                contract="WcfService.IWcfContract" name="BasicHttpBinding_IWcfContract" />
           <endpoint address="http://localhost:8733/Design_Time_Addresses/OQAService/"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IOQAContract"
                contract="OQAService.IOQAContract" name="BasicHttpBinding_IOQAContract" />
        </client>
    </system.serviceModel>
</configuration>